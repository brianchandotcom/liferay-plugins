<?xml version="1.0"?>
<!DOCTYPE project>

<project name="build-common-osgi-plugin">
	<import file="build-common-plugin.xml" />

	<target name="jar">
		<jar-macro
			module.dir="."
		/>
	</target>

	<target name="release" depends="war">
		<release-macro
			module.dir="."
		/>
	</target>

	<target name="test" depends="compile" if="Test-Cases">
		<test-osgi-macro
			module.dir="."
		/>
	</target>
</project>